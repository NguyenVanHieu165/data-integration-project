APP_ENV=development
LOG_LEVEL=INFO
LOG_DIR=logs

# Source Database: ComVanPhong (Data Source 2)
SOURCE_DB_HOST=localhost
SOURCE_DB_PORT=1433
SOURCE_DB_NAME=ComVanPhong
SOURCE_DB_TRUSTED_CONNECTION=true

# Target Database: newdata (Đích cuối cùng)
TARGET_DB_HOST=localhost
TARGET_DB_PORT=1433
TARGET_DB_NAME=newdata
TARGET_DB_TRUSTED_CONNECTION=true

# RabbitMQ
# AMQP Port: 5672 (for connections)
# Management UI: http://localhost:15672 (guest/guest)
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
